{
  "name": "docker-sandbox",
  "version": "1.0.0",
  "description": "A sandbox to play with docker, node, and mongo",
  "main": "server.js",
  "scripts": {
    "test-unit": "NODE_ENV=test mocha tests/*.js",
    "start": "tsc && concurrently \"tsc -w\" \"nodemon api/server.js\"",
    "docker:rm": "docker rm $(docker ps -aq)",
    "connect:node": "docker-compose run --rm node /bin/bash",
    "jscs": "jscs api/*.js api/**/*.js",
    "lint": "eslint --color --fix api/*.js api/**/*.js",
    "postinstall": "typings install",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings"
  },
  "author": "Jason Jones",
  "license": "MIT",
  "dependencies": {
    "@angular/common": "~2.0.2",
    "@angular/compiler": "~2.0.2",
    "@angular/core": "~2.0.2",
    "@angular/forms": "~2.0.2",
    "@angular/http": "~2.0.2",
    "@angular/platform-browser": "~2.0.2",
    "@angular/platform-browser-dynamic": "~2.0.2",
    "@angular/router": "~3.0.2",
    "@angular/upgrade": "~2.0.2",
    "angular-in-memory-web-api": "~0.2.0",
    "bootstrap": "^3.3.7",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.15.2",
    "chalk": "^1.1.3",
    "core-js": "^2.4.1",
    "ejs": "^2.5.2",
    "express": "^4.14.0",
    "jsonwebtoken": "^7.1.9",
    "mongoose": "^4.6.0",
    "morgan": "^1.7.0",
    "reflect-metadata": "^0.1.8",
    "rxjs": "5.0.0-beta.12",
    "systemjs": "0.19.39",
    "zone.js": "^0.6.25"
  },
  "devDependencies": {
    "browser-sync": "^2.18.2",
    "chai": "^3.5.0",
    "concurrently": "^3.0.0",
    "eslint": "^3.7.1",
    "mocha": "^3.1.2",
    "nodemon": "^1.10.2",
    "sinon": "^1.17.6",
    "typescript": "^2.0.3",
    "typings":"^1.4.0"
  }
}
