{
  "name": "docker-sandbox",
  "version": "1.0.0",
  "description": "A sandbox to play with docker, node, and mongo",
  "main": "server.js",
  "scripts": {
    "test-unit": "NODE_ENV=test mocha tests/*.js",
    "start": "nodemon api/server.js",
    "docker:rm": "docker rm $(docker ps -aq)",
    "connect:node": "docker-compose run --rm node /bin/bash",
    "jscs": "jscs api/*.js api/**/*.js",
    "lint": "eslint --color --fix api/*.js api/**/*.js"
  },
  "author": "Jason Jones",
  "license": "MIT",
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.15.2",
    "chalk": "^1.1.3",
    "ejs": "^2.5.2",
    "express": "^4.14.0",
    "jsonwebtoken": "^7.1.9",
    "mongoose": "^4.6.0",
    "morgan": "^1.7.0"
  },
  "devDependencies": {
    "browser-sync": "^2.18.2",
    "chai": "^3.5.0",
    "eslint": "^3.7.1",
    "mocha": "^3.1.2",
    "nodemon": "^1.10.2",
    "sinon": "^1.17.6"
  }
}
