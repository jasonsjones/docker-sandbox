{
  "name": "docker-sandbox",
  "version": "1.0.0",
  "description": "A sandbox to play with docker, node, and mongo",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "nodemon api/server.js",
    "docker:rm": "docker rm $(docker ps -aq)",
    "connect:node": "docker-compose run --rm node /bin/bash",
    "jscs": "jscs api/*.js api/**/*.js",
    "lint": "eslint api/*.js"
  },
  "author": "Jason Jones",
  "license": "MIT",
  "dependencies": {
    "ejs": "^2.5.2",
    "express": "^4.14.0",
    "mongoose": "^4.6.0"
  },
  "devDependencies": {
    "eslint": "^3.7.1",
    "jscs": "^3.0.7",
    "nodemon": "^1.10.2"
  }
}
